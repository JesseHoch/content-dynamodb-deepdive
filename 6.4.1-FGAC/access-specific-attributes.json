{
  "Version": "2012-10-17",
  "Statement": [{
    "Sid": "LimitAccessToSpecificAttributes",
    "Effect": "Allow",
    "Action": [
      "dynamodb:GetItem",
      "dynamodb:Query",
      "dynamodb:BatchGetItem",
      "dynamodb:Scan"
    ],
    "Resource": [
      "arn:aws:dynamodb:us-east-1:408073669315:table/pinehead_records_s3"
    ],
    "Condition": {
      "ForAllValues:StringEquals": {
        "dynamodb:Attributes": [
          "type",
          "id",
          "album_art",
          "artist_id",
          "format",
          "name_title",
          "sku",
          "year"
        ]
      },
      "StringEqualsIfExists": {
        "dynamodb:Select": "SPECIFIC_ATTRIBUTES"
      }
    }
  }]
}